{
  "build": {
    "beforeDevCommand": "pnpm dev",
    "beforeBuildCommand": "pnpm build",
    "devPath": "http://localhost:1420",
    "distDir": "../dist",
    "withGlobalTauri": false
  },
  "package": {
    "productName": "StockPannel",
    "version": "../package.json"
  },
  "tauri": {
    "updater": {
      "active": true,
      "dialog": true,
      "pubkey": "dW50cnVzdGVkIGNvbW1lbnQ6IG1pbmlzaWduIHB1YmxpYyBrZXk6IEQwQkMxM0FBMTNFOTlGQzMKUldURG4ra1RxaE84MExyd2ZqdUtYSy9jQ2o5c0hJN2dYaWVNZTZQYURFY2o3TVljYTcvR3FzNDMK",
      "endpoints": [
        "https://github.com/cbingb666/stock-pannel/releases/latest/download/latest.json"
      ]
    },
    "allowlist": {
      "all": false,
      "app": {
        "all": true
      },
      "window": {
        "setTitle": true
      },
      "shell": {
        "all": false,
        "open": true
      }
    },
    "bundle": {
      "active": true,
      "targets": "all",
      "identifier": "com.stock-pannel.build",
      "icon": [
        "icons/icon.png",
        "icons/icon.icns",
        "icons/icon.ico"
      ],
      "windows": {
        "nsis": {
          "languages": [
            "SimpChinese",
            "English"
          ]
        },
        "wix": {
          "language": [
            "zh-CN",
            "en-US"
          ]
        }
      }
    },
    "security": {
      "csp": null
    },
    "windows": [
      {
        "fullscreen": false,
        "resizable": true,
        "title": "StockPannel",
        "width": 800,
        "height": 600
      }
    ]
  }
}